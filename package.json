{
  "name": "nodejs-typescript-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -f ./*.js; rm -f ./*.js.map; rm -f ./intermediates/*.js; rm -f ./intermediates/*.js.map",
    "tsc": "./node_modules/.bin/tsc",
    "tsc:w": "./node_modules/.bin/tsc -w",
    "start": "node ./build/www",
    "go": "concurrent \"npm run tsc:w\" \"npm run serve\" "
  },
  "dependencies": {
    "serve-favicon": "~2.3.0",
    "tslint": "3.7.0-dev.2",
    "typings": "0.8.1"
  },
  "devDependencies": {
    "typescript": "1.7.3"
  }
}
